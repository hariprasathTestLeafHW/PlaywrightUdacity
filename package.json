{
  "name": "cucumber-playwright-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "concurrently --kill-others-on-fail \"ng lint\" \"npm run lint:scss\" \"npm run lint:prettier\"",
    "lint:eslint": "ng lint",
    "lint:prettier": "prettier --check \"./**/*.{js,json,md,ts,html,scss}\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "fix:prettier": "prettier --write \"./**/*.{js,json,md,ts,html,scss}\"",
    "e2e:serve": "ng serve --configuration=e2e",
    "e2e:ci": "start-server-and-test e2e:serve http://localhost:4200 e2e:test-ci",
    "e2e:ci-and-report": "npm-run-all --serial --continue-on-error e2e:ci e2e:report-publish",
    "e2e:test-ci": "npx cucumber-js -p ci",
    "e2e:test-local": "npx cucumber-js",
    "e2e:test-local-tags": "npx cucumber-js --tags @scenario",
    "e2e:report-publish": "node e2e/reporting/index.js",
    "e2e:install": "npx playwright install",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.0.5",
    "@angular/compiler-cli": "^15.0.0",
    "@cucumber/cucumber": "^9.0.1",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@playwright/test": "^1.31.2",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "concurrently": "^8.2.2",
    "cucumber-html-reporter": "^6.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.31.2",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^16.2.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.2"
  }
}
